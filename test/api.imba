var endpoints = {
	'/customers.json': customers
	'/orders.json': orders
}

export def rpc url
	Promise.new do |resolve,reject|
		let data = endpoints[url]

		if let cid = url.match(/\/customers\/(\w+)\/orders.json/)
			console.log "get custom order data"
			data = orders.filter do |order|
				order:custid == cid[1]

		if data
			setTimeout(&,50) do resolve(data)
		else
			resolve(null)
			